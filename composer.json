{
    "name": "uteq/laravel-move",
    "description": "Laravel admin panel powered by Livewire and Jetstream",
    "keywords": [
        "uteq",
        "laravel-move"
    ],
    "homepage": "https://github.com/uteq/laravel-move",
    "license": "MIT",
    "authors": [
        {
            "name": "Nathan Jansen",
            "email": "info@uteq.nl",
            "homepage": "https://uteq.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "illuminate/contracts": "^8.0",
        "intervention/image": "^2.5",
        "laravel/jetstream": "^1.5",
        "livewire/livewire": "^2.3.0",
        "maatwebsite/excel": "^3.1",
        "spatie/data-transfer-object": "^1.13",
        "spatie/laravel-medialibrary": "^8.7",
        "spatie/laravel-searchable": "^1.7"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8",
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Uteq\\Move\\": "src",
            "Uteq\\Move\\Database\\Factories\\": "database/factories"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Uteq\\Move\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uteq\\Move\\MoveServiceProvider"
            ],
            "aliases": {
                "Move": "Uteq\\Move\\Facades\\Move"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
